<!DOCTYPE html>
<html>
<head>
  <title>Volatility App</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "http://fonts.googleapis.com/css?family=Gudea" %>
  <%= stylesheet_link_tag    "//netdna.bootstrapcdn.com/font-awesome/3.0.2/css/font-awesome.css" %>
  <%= javascript_include_tag "application" %>
  <%= yield :head %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'shared/header' %>

  <div class="main-container">
    
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
    <div class="main-content">

      <%= yield %>

    </div>


    


  </div>

<%= render 'shared/footer' %>
</body>




</html>
